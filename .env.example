# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=fas_smart
DB_USER=tradingbot
DB_PASSWORD=your_password_here

# Binance (optional, for private endpoints)
# BINANCE_API_KEY=
# BINANCE_API_SECRET=

# Thresholds
ENTRY_VOLUME_THRESHOLD=100000
EXIT_VOLUME_THRESHOLD=30000

# Logging
LOG_LEVEL=INFO

# IPv6 Rotation (for API rate limit bypass)
# IPV6_ENABLED=true
# IPV6_PREFIX=2001:db8::
# IPV6_PREFIX_LENGTH=64

# Proxy rotation (loads from proxy.txt file)
# PROXY_ENABLED=true
# PROXY_PARALLEL=30
# Add proxies to proxy.txt in format: ip:port (one per line)

# Warmup settings
# RESTORE_HOURS=168  # How many hours of data to restore on startup (default: 168 = 7 days)
